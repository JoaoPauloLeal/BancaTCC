@model BancaTcc.Models.Disciplinas
@{
    ViewBag.Title = "CreateDisciplina";
}

<h2>CreateDisciplina</h2>

<div class="panel">
    <form action="~/Disciplina/CreateDisciplina" method="post">
        <div class="form-group">
            <label>Id</label>
            <input class="form-control" type="number" id="Id" name="Id" placeholder="Identificador da Disciplina" />
        </div>

        <div class="form-group">
            <label>Nome</label>
            <input class="form-control" type="text" id="Nome" name="Nome" placeholder="Nome da Disciplina" />
        </div>

        <button type="submit" class="btn btn-default">Cadastrar Disciplina</button>
    </form>
</div>